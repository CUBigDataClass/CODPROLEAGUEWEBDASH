{
  "name": "trip-planner",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "client-build": "cd client && npm run build",
    "server": "nodemon server.js",
    "build": "npm run build",
    "dev": "concurrently --kill-others-on-fail \"node server\" \"npm run client\"",
    "postinstall": "cd client && npm install --only=dev && npm install && npm run build",
    "start": "node server.js",
    "start-local": "npm run client-build && npm run server"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.11.0",
    "aws-elasticsearch-connector": "^9.0.3",
    "aws-sdk": "^2.864.0",
    "aws-xray-sdk": "^3.2.0",
    "aws4": "^1.11.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "http-aws-es": "^6.0.0",
    "missing": "0.0.1",
    "node-cron": "^2.0.3",
    "react-bootstrap": "^1.5.1",
    "request": "^2.88.2",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
